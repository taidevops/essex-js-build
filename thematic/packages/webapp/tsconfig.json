{
	"compilerOptions": {
        "target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"declaration": true,
		"jsx": "react-jsx",
		"lib": ["dom", "esnext"],
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"noImplicitAny": false
	},
    "exclude": ["**/node_modules/**", "**/__tests__/**"]
}